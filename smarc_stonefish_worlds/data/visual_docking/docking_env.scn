    <specs radius="0.1" cone_angle="180.0" illuminance="2000.0"/>
    <color temperature="5600.0"/>
    <link name="Link1"/>
    <!--world_transform xyz="$(param marked_positions/light_center/position)" rpy="$(param marked_positions/light_center/orientation)"/-->
    <world_transform xyz="20.25 -0.25 1.75" rpy="0.0 -1.5708 0.0"/>
    <!--<world_transform xyz="20.25 -0.25 -0.25" rpy="0.0 -1.5708 0.0"/>-->
</light>

<static name="Light_Bottom_Left_Buoy" type="model">
    <look name="yellow"/>
	<material name="Neutral"/>
    <world_transform xyz="20.50 -0.25 1.75" rpy="0.0 -1.5708 0.0"/>
    <!--<world_transform xyz="20.50 -0.25 -0.25" rpy="0.0 -1.5708 0.0"/>-->
	<physical>
		<mesh filename="$(param mesh/a0_buoy)" scale="0.5"/>
		<origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
	</physical>
</static>

<light name="Light_Bottom_Right">
    <specs radius="0.1" cone_angle="180.0" illuminance="2000.0"/>
    <color temperature="5600.0"/>
    <link name="Link1"/>
    <!--world_transform xyz="$(param marked_positions/light_center/position)" rpy="$(param marked_positions/light_center/orientation)"/-->
    <world_transform xyz="20.25 0.25 1.75" rpy="0.0 -1.5708 0.0"/>
    <!--<world_transform xyz="20.25 0.25 -0.25" rpy="0.0 -1.5708 0.0"/>-->
</light>

