    <look name="$(param marked_positions/buoy_corner1/color)"/>
	<material name="Neutral"/>
    <world_transform xyz="$(param marked_positions/buoy_corner1/position)" rpy="$(param marked_positions/buoy_corner1/orientation)"/>
